/* Basic C program for calculate the sum of two numbers */
 
#include <stdio.h>   
int total = 0;      /* initialize total with 0 
int sum (int, int);  /* declare sum variable 
int main ()          
{
    printf ("This is a C basic program \n");  
    total = sum (1, 1);
    printf ("Sum of two numbers : %d \n", total);
    return 0;
}
 
int sum (int a, int b) 
{   
    return a + b;      
}
